{
  "name": "event-kit",
  "version": "1.5.0",
  "description": "Simple library for implementing and consuming evented APIs",
  "main": "./src/event-kit",
  "scripts": {
    "prepublish": "npm run lint && npm run atomdoc",
    "atomdoc": "script/generate-api-docs",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "eslint .",
    "fix-all": "eslint --fix .",
    "test": "npm run lint && jasmine JASMINE_CONFIG_PATH=spec/jasmine.json",
    "ci": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atom/event-kit.git"
  },
  "bugs": {
    "url": "https://github.com/atom/event-kit/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/atom/event-kit/raw/master/LICENSE.md"
    }
  ],
  "dependencies": {
    "assert": "^1.4.1",
    "grim": "^1.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "7.1.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "3.9.1",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-flowtype": "2.25.0",
    "eslint-plugin-jasmine": "1.8.1",
    "eslint-plugin-prefer-object-spread": "1.1.0",
    "eslint-plugin-react": "6.4.1",
    "flow-bin": "^0.34.0",
    "fs-extra": "^1.0.0",
    "glob": "^7.1.1",
    "jasmine": "^2.5.2",
    "joanna": "0.0.6",
    "rimraf": "^2.2.2",
    "tello": "^1.0.6",
    "temp": "^0.6.0"
  }
}
